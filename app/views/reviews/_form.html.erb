<%= form_for [@beach, @review] do |f| %>
  <div>
    <%= f.label :rating, "Rating" %>
    <%= f.select :rating, [['', nil], ['Terrible - 1', 1], ['Bad - 2', 2], ['Mediocre - 3', 3], ['Good - 4', 4], ['Great - 5', 5]]%>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <%= f.submit "Submit" %>
<% end %>
